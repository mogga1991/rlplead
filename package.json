{
  "name": "fedleads",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "worker": "tsx worker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test --config=tests/config/playwright.config.ts",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test tests/e2e --config=tests/config/playwright.config.ts",
    "test:integration": "playwright test tests/integration --config=tests/config/playwright.config.ts",
    "test:smoke": "playwright test tests/smoke.spec.ts --config=tests/config/playwright.config.ts",
    "test:ui": "playwright test --ui --config=tests/config/playwright.config.ts",
    "test:headed": "playwright test --headed --config=tests/config/playwright.config.ts",
    "test:debug": "playwright test --debug --config=tests/config/playwright.config.ts",
    "test:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test",
    "test:apify": "node scripts/test-apify.js",
    "test:db": "node scripts/test-db.js",
    "test:flow": "node scripts/test-full-flow.js",
    "test:auth": "node scripts/test-auth.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "apify-client": "^2.22.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.67.2",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "ioredis": "^5.9.2",
    "lucide-react": "^0.462.0",
    "next": "^14.2.18",
    "next-auth": "^5.0.0-beta.30",
    "rate-limiter-flexible": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.18"
  }
}
